
<div class="col-sm-9">

   <div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Friends</h3>
  </div>
  <div class="panel-body">

<table>
  <thead>
    <tr>
      <th>Name</th>

    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= gravatar_for user %><td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      

  
      </tr>
    <% end %>
  </tbody>
</table>

  </div>
</div>


